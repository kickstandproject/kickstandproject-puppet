[access "refs/heads/*"]
        label-Code-Review = -2..+2 group kickstandproject-ci-admins
        label-Approved = +0..+1 group kickstandproject-ci-admins
[access "refs/tags/*"]
        create = group kickstandproject-ci-admins
        pushTag = group kickstandproject-ci-admins
[project]
        state = active
[receive]
        requireChangeId = true
[submit]
        action = cherry pick
        mergeContent = true
